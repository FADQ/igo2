    "gulp": "gulp",
    "ng": "ng",
    "start": "gulp watch:locale | ng serve interne --host 0.0.0.0 --port 4200 --proxy-config proxy.conf.json",
    "pes.start": "ng serve pes --host 0.0.0.0 --proxy-config proxy.conf.json",
    "pes.build.prod": "ng build pes --configuration production --base-href /igo2_pes/",
    "pes_interne.start": "ng serve pes_interne --host 0.0.0.0 --proxy-config proxy.conf.json",
    "pes_interne.build.prod": "ng build pes_interne --configuration production --base-href /igo2_pes_interne/",
    "build": "ng build",
    "lint": "ng lint",
    "lint.fix": "ng lint --fix",
    "e2e": "ng e2e --port 4300",
    "build.prod": "ng build interne --configuration production --base-href /igo2/",
    "serve.prod": "node_modules/live-server/live-server.js ./dist/interne/ --port=4200 --no-browser",
    "doc": "compodoc -p src/tsconfig.interne.json -s --port 4220",
    "build.doc": "compodoc -p src/tsconfig.interne.json",
    "serve.doc": "compodoc -s --port 4220",
    "coverage": "npm run build.coverage && npm run serve.coverage",
    "build.coverage": "ng test --code-coverage --watch=false",
    "serve.coverage": "node_modules/live-server/live-server.js ./coverage/ --port=4210 --no-browser",
    "i18n": "ng2-translate-extract --dir ./src --output ./src/assets/locale/ --format=json --clean",
    "pretest": "ng lint",
    "test": "ng test --watch=false --browsers=ChromeHeadless",
    "test.watch": "ng test",
    "test.all": "npm run test && npm run e2e",
    "clean": "rimraf ./dist && rimraf ./node_modules && npm cache clean --force && git checkout -- package.json && git checkout -- package-lock.json",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog-current": "conventional-changelog -p angular -r 2",
    "preversion": "npm run test.all && rimraf ./dist && npm run build.prod",
    "version": "npm run changelog && git add -A CHANGELOG.md"
  },
